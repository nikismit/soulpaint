<?xml version="1.0" encoding="UTF-8"?>

<manifest android:installLocation="auto" xmlns:android="http://schemas.android.com/apk/res/android">

  <uses-feature android:required="true" android:version="1" android:name="android.hardware.vr.headtracking"/>

  <!-- Request the headset DoF mode -->



  -<application>


    -<activity android:name="com.unity3d.player.UnityPlayerActivity" android:excludeFromRecents="true" android:launchMode="singleTask" android:configChanges="locale|fontScale|keyboard|keyboardHidden|mcc|mnc|navigation|orientation|screenLayout|screenSize|smallestScreenSize|touchscreen|uiMode" android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen">


      -<intent-filter>

        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>

      </intent-filter>

    </activity>

    <meta-data android:name="unityplayer.SkipPermissionsDialog" android:value="false"/>

  </application>

</manifest>